{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/ibm/ILOG/CPLEX_Studio1210/cplex/include/**",
                "/opt/ibm/ILOG/CPLEX_Studio1210/concert/include/**",
                "/usr/include/**",
                "/usr/local/include/**",
                "/usr/local/lib"
            ],
            "defines": [
                "fPIC",
                "fexceptions",
                "DNDEBUG",
                "DIL_STD",
                "IL_STD"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-x86",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-m64",
                "-O",
                "-fPIC",
                "-fno-strict-aliasing",
                "-fexceptions",
                "-DNDEBUG",
                "-DIL_STD",
                "-Wno-ignored-attributes "
            ]
        }
    ],
    "version": 4
}